{
  "compilerOptions": {
    "target": "es5",
    "esModuleInterop": true,
    "lib": [
      "dom",
      "es5",
      "es6",
      "es2016.array.include",
      "es2015.promise",
      "es2015",
      "webworker"
    ],
    "rootDirs": ["./src"],
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist/lib",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    // "strict": true,
    // "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true
  }
}
